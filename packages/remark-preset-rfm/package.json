{
  "name": "@ricora/remark-preset-rfm",
  "version": "0.0.0",
  "description": "remark preset to support RFM",
  "private": false,
  "keywords": [
    "markdown",
    "mdx",
    "unified",
    "remark",
    "remark-preset",
    "rfm",
    "ricora"
  ],
  "author": "ricora",
  "license": "MIT",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ricora/ricora-flavored-markdown.git"
  },
  "bugs": {
    "url": "https://github.com/ricora/ricora-flavored-markdown/issues"
  },
  "homepage": "https://github.com/ricora/ricora-flavored-markdown",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "src",
    "dist",
    "!**/*.test.ts",
    "!**/*.tsbuildinfo",
    "README.md",
    "LICENSE"
  ],
  "engines": {
    "node": ">=16"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "rimraf dist && tsc --project tsconfig.build.json",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "release": "bun run build && bun run test:ci && bun run changeset publish",
    "prepublishOnly": "bun run build"
  },
  "peerDependencies": {},
  "dependencies": {
    "@r4ai/remark-embed": "0.3.1",
    "defu": "6.1.4",
    "hast-util-from-html-isomorphic": "2.0.0",
    "remark-gfm": "4.0.0"
  },
  "devDependencies": {
    "@testing-library/dom": "10.4.0",
    "@testing-library/jest-dom": "6.5.0",
    "@tsconfig/node16": "16.1.3",
    "@types/bun": "1.1.8",
    "@types/hast": "3.0.4",
    "@types/mdast": "4.0.4",
    "@vitest/coverage-v8": "2.0.5",
    "dedent": "1.5.3",
    "happy-dom": "15.7.3",
    "msw": "2.4.3",
    "rehype-raw": "7.0.0",
    "rehype-stringify": "10.0.0",
    "remark-math": "6.0.0",
    "remark-rehype": "11.1.0",
    "rimraf": "6.0.1",
    "ts-essentials": "10.0.2",
    "typescript": "5.6.2",
    "unified": "11.0.5",
    "vitest": "2.0.5"
  },
  "packageManager": "bun@1.1.17"
}
